{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Ember Language Server (VS Code)",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "${execPath}",
      "args": [
        "--extensionDevelopmentPath=/home/preston/.vscode/extensions/lifeart.vscode-ember-unstable-1.3.2"
      ],
      "stopOnEntry": false,
      "sourceMaps": true,
      "outFiles": [
        "/home/preston/.vscode/extensions/lifeart.vscode-ember-unstable-1.3.2/out/src/**/*.js"
      ]
    },
    {
      "name": "Attach to Ember Language Server (VS Code)",
      "type": "node",
      "request": "attach",
      "port": 6004,
      "sourceMaps": true,
      "protocol": "inspector",
      "outFiles": [
        "/home/preston/.vscode/extensions/lifeart.vscode-ember-unstable-1.3.2/node_modules/@emberwatch/ember-language-server/lib/**/*.js"
      ],
      "restart": true,
      "smartStep": true
    }
  ]
}
